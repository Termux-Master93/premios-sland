---
import Layout from "@layouts/Layout.astro";
import HeroContainers from "@components/HeroContainers.astro";
import Button from "../Button.astro";
import Numbers from "@components/Numbers.astro";
---

<Layout title="archivo" description="archivo">
    <HeroContainers img="/img/bg-archivo.webp">
        <h1 class="text-5xl font-semibold mb-20 tracking-widest">Ediciones Anteriores</h1>
        <Button 
            class="animate-fade-up z-10"
            url="#">Revive la Ultima Gala</Button>
    </HeroContainers>
    <main>
        <Numbers/>
    </main>
</Layout>
